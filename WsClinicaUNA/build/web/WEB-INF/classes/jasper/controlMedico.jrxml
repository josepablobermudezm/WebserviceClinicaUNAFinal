	<queryString>
		<![CDATA[SELECT TO_CHAR(c.cnt_fecha,'yyyy/mm/dd') as Fecha,TO_CHAR(c.cnt_hora, 'HH24:MI') as Hora,c.cnt_presion as Presion,c.cnt_frecuencia_cardiaca as 
